        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "isolatedModules": true,
        "outDir": "build/"
    },
    "exclude": [
        "node_modules",
        "typings"
    ]
}